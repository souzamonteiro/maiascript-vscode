{
    "name": "maiascript",
    "displayName": "MaiaScript",
    "description": "MaiaScript programming language syntax highlight for Visual Studio Code.",
    "version": "1.2.0",
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "maia",
            "aliases": ["MaiaScript", "gua"],
            "extensions": ["maia"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "maia",
            "scopeName": "source.maiascript",
            "path": "./syntaxes/maia.tmLanguage.json"
        }]
    }
}
